           SPELL=gens
         VERSION=2.12a
          SOURCE=$SPELL-rc2.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-linux-$VERSION
   SOURCE_URL[0]=${SOURCEFORGE_URL}/$SPELL/$SOURCE
        WEB_SITE=http://gens.consolemul.com/
          MD5[0]=dce6e4e9290b77603ceb9e91c66711e9
      LICENSE[0]=GPL
         ENTERED=20040302
