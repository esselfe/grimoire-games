mk_source_dir        $SOURCE_DIRECTORY     &&
unpack_file    &&
unpack_file 2  &&

cd  ${SOURCE_DIRECTORY}                 &&

if  [  !  -z  $SOURCE3  ];  then
  unpack_file 3
fi                                      &&

if  [  !  -z  $SOURCE3  ];  then
  unpack_file 4
fi                                      &&

if  [  !  -z  $SOURCE4  ];  then
  unpack_file 5
fi
