mk_source_dir        $SOURCE_DIRECTORY  &&
unpack               $SOURCE ${MD5[0]}  &&

cd  ${SOURCE_DIRECTORY}                 &&
if  [  !  -z  $SOURCE2  ]
then
  unpack  ${SOURCE2}  ${MD5[1]}
fi                                      &&

if  [  !  -z  $SOURCE3  ]
then
  unpack  ${SOURCE3}  ${MD5[2]}
fi                                      &&

if  [  !  -z  $SOURCE4  ]
then
  unpack  ${SOURCE4}  ${MD5[3]}
fi                                      &&
cd  ..
