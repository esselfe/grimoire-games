mk_source_dir $SOURCE_DIRECTORY     &&
cd $SOURCE_DIRECTORY                &&
unpack_file             &&
mv $SPELL-$VERSION $SPELL           &&
unpack_file 2           &&
mv $SPELL-$VERSION-data data        &&
unpack_file 3           &&
mv $SPELL-$VERSION-vssetup vssetup  &&
unpack_file 4           &&
mv $SPELL-$VERSION-music data/music 
