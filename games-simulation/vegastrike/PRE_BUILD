mk_source_dir $SOURCE_DIRECTORY     &&
cd $SOURCE_DIRECTORY                &&
unpack_file                         &&
unpack_file 2                       &&
cd ${SOURCE_DIRECTORY}/vegastrike   &&
if [[ $VEGASTRIKE_DEVEL == "y" ]]; then
./bootstrap-sh
else 
aclocal        &&
autoconf   -f  &&
autoheader -f  &&
automake  -acf
fi
