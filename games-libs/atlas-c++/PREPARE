config_query_list  ATLAS_CPP_BRANCH  "Which branch do you want?"  \
                   stable  devel  cvs                             &&

if  [  "$ATLAS_CPP_BRANCH"  ==  "cvs"  ];  then
  config_query  ATLAS_CPP_AUTOUPDATE                               \
                'Automatically update on every system-update?'  n
fi
